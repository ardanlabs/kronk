{
  "$schema": "https://opencode.ai/config.json",
  "model": "kronk/gpt-oss-120b-F16/IMC",
  "enabled_providers": ["kronk"],
  "permission": "allow",
  "mcp": {
    "Kronk": {
      "type": "remote",
      "url": "http://localhost:9000/mcp",
      "enabled": true,
      "timeout": 60000
    }
  },
  "provider": {
    "kronk": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "Kronk (local)",
      "options": {
        "baseURL": "http://localhost:8080/v1",
        "apiKey": "123"
      },
      "models": {
        "gpt-oss-120b-F16/IMC": {
          "name": "GPT-OSS 120B F16/IMC",
          "limit": {
            "context": 131072,
            "output": 65536
          },
          "reasoning": true,
          "options": {
            "reasoningEffort": "medium"
          }
        }
      }
    }
  }
}